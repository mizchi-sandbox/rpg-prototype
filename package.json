{
  "name": "rpg-prototype",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "ava": {
    "babel": "inherit",
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "clean": "rimraf src/master/*.js public/bundle.js",
    "prepare": "yarn run clean; yarn build:master",
    "deploy": "yarn prepare; yarn build:src:prod; yarn gh-pages -- -d public/",
    "build:master": "node script/gen-code.js",
    "watch:master": "chokidar masterdata/**/* -c 'yarn build:master' --polling",
    "build:src": "webpack",
    "build:src:prod": "NODE_ENV=production webpack",
    "watch:src": "webpack-dev-server",
    "watch": "yarn prepare; run-p watch:*",
    "test": "ava",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s public"
  },
  "dependencies": {
    "aphrodite": "^1.2.1",
    "babel-runtime": "^6.23.0",
    "prop-types": "^15.5.8",
    "react": "next",
    "react-dom": "next",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-tooltip": "^3.3.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-saga": "^0.15.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@mizchi/babel-preset": "^1.1.1",
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-env": "^1.4.0",
    "change-case": "^3.0.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-mizchi": "^1.0.2",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-mutation": "^1.0.0",
    "flow-bin": "^0.45.0",
    "gh-pages": "^0.12.0",
    "json-schema-to-flow-type": "^0.2.6",
    "npm-run-all": "^4.0.2",
    "react-hot-loader": "next",
    "require-yaml": "^0.0.1",
    "rimraf": "^2.6.1",
    "tv4": "^1.3.0",
    "webpack": "2.2",
    "webpack-dev-server": "2.3"
  }
}
